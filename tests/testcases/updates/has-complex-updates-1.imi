var

  b : bool;
  x, y : clock;

automaton P

  loc s0: invariant True
  when True

  do {
    seq
      b := False;
    then
      x :=0,
      y :=0,
      b := True

  }
  goto lend;

  accepting loc lend : invariant True

end


init := {
  discrete =
    loc[P]:=s0,
    b := True
  ;

}
