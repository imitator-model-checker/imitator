var

  i : int;
  s1 : int stack;



fn f1() : int
begin
  var x : int = stack_pop(s1);
  return 1 + 0
end

fn f2() : int
begin
  return f1()
end

fn f3() : int
begin
  return 0
end

fn f4() : int
begin
  return f1() + f3()
end

fn f5(i : int) : int begin
  return i + 1
end

automaton P

  loc s0: invariant True
  when True

  do {
    mix
      i := f1() + f2() + f3() + f4() + f5(0);
  }
  goto lend;

  accepting loc lend : invariant True

end


init := {
  discrete =
    loc[P]:=s0,
    s1 := stack(),
    i := 0
  ;

}
