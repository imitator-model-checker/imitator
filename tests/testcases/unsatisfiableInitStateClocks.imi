var

(* Clocks *)
 	x, y,
		: clock;


(************************************************************)
  automaton pta
(************************************************************)
synclabs: a;

loc l1: invariant True
	when x >= y sync a do {x := y} goto l1;

end (* pta *)



(************************************************************)
(* Initial state *)
(************************************************************)

init :=
	(*------------------------------------------------------------*)
	(* Initial location *)
	(*------------------------------------------------------------*)
	& loc[pta] = l1

	(*------------------------------------------------------------*)
	(* Initial clock constraints *)
	(*------------------------------------------------------------*)
	& x > y
	& x = 2
	& y >= 3
;


(************************************************************)
(* The end *)
(************************************************************)
end
